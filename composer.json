{
    "name"             : "pulsarcode/framework",
    "description"      : "Pulsarcode Framework",
    "repositories"     : [
        {
            "type": "git",
            "url" : "https://github.com/tecnocat/LswDoctrinePdoDblib"
        }
    ],
    "require"          : {
        "php"                        : ">=5.3.9",
        "symfony/dom-crawler"        : "2.7.*",
        "symfony/form"               : "2.7.*",
        "symfony/http-foundation"    : "2.7.*",
        "symfony/routing"            : "2.7.*",
        "symfony/twig-bridge"        : "2.7.*",
        "symfony/validator"          : "2.7.*",
        "symfony/yaml"               : "2.7.*",
        "doctrine/cache"             : "1.3.*",
        "doctrine/dbal"              : "2.3.*",
        "phpmailer/phpmailer"        : "5.2.*",
        "sami/sami"                  : "2.0.*",
        "twig/twig"                  : "1.22.*",
        "leaseweb/doctrine-pdo-dblib": "1.0.*"
    },
    "autoload"         : {
        "psr-0": {
            "Pulsarcode\\Framework": "src/"
        }
    },
    "license"          : "MIT",
    "authors"          : [
        {
            "name" : "Alpha Nieves",
            "email": "alpha@pulsarcode.com"
        }
    ],
